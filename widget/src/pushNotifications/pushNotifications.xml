<?xml version="1.0" encoding="utf-8"?>
<widget id="pushNotifications.widget.pushNotifications" needsEntityContext="false" offlineCapable="true" xmlns="http://www.mendix.com/widget/1.0/">
	<name>Push Notifications</name>
	<description>Use Phone gap to recieve push notifications</description>
	<icon></icon>
	<properties>
		<property key="notificationActions" type="object" isList="true" required="false">
			<caption>Actions</caption>
			<category>Notification Actions</category>
			<description>The application is opened by default, or specify an action for the notification</description>
			<properties>
				<property key="actionName" type="string">
					<caption>Action name</caption>
					<category>Action</category>
					<description>Unique identifier for the action (Should match the notification action name provided in the 'Send message' action)</description>
				</property>
				<property key="actionType" type="enumeration" defaultValue="openPage">
					<caption>Action</caption>
					<category>Action</category>
					<description/>
					<enumerationValues>
						<enumerationValue key="openPage">Open page</enumerationValue>
						<enumerationValue key="callMicroflow">Call microflow</enumerationValue>
					</enumerationValues>
				</property>
				<property key="contextEntity" type="entity" isPath="no" required="false">
					<caption>Entity</caption>
					<category>Action</category>
					<description>The entity of the object used as context of the page or the microflow parameter</description>
				</property>
				<property key="page" type="form" required="false">
					<caption>Page</caption>
					<category>Action</category>
					<description/>
				</property>
				<property key="microflow" type="microflow" entityProperty="contextEntity" required="false">
					<caption>Microflow</caption>
					<category>Action</category>
					<description/>
					<returnType type="Void"/>
				</property>
			</properties>
		</property>
		<property key="synchronizationMessage" type="translatableString">
			<caption>Synchronization message</caption>
			<category>Messages</category>
			<description>The message that will be shown when the referred object is not yet available in the offline app.</description>
			<translations>
				<translation lang="en_US">The notification refers to data that you do not have on your device yet. Do you want to synchronize?</translation>
			</translations>
		</property>
	</properties>
</widget>
